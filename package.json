{
    "name": "vuepress-plugin-code-doc",
    "version": "0.0.1-alpha",
    "description": " Writing Documents Like Element-ui",
    "main": "src/index.js",
    "scripts": {
        "test": "cross-env NODE_ENV=test jest",
        "docs": "vuepress dev docs",
        "docs:build": "vuepress build docs"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-add": "lint-staged",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{test,src}/**/*.{js}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jardenliu/vuepress-plugin-code-doc.git"
    },
    "keywords": [
        "vuepress",
        "code",
        "demo",
        "doc",
        "element",
        "ui",
        "components"
    ],
    "author": "jardenliu",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jardenliu/vuepress-plugin-code-doc/issues"
    },
    "homepage": "https://github.com/jardenliu/vuepress-plugin-code-doc#readme",
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.2",
        "cross-env": "^5.2.0",
        "eslint": "^6.1.0",
        "eslint-config-standard": "^13.0.1",
        "eslint-config-vue": "^2.0.2",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^5.2.3",
        "husky": "^3.0.1",
        "jest": "^24.8.0",
        "jest-serializer-vue": "^2.0.2",
        "jest-transform-stub": "^2.0.0",
        "lint-staged": "^9.2.0",
        "prettier": "^1.18.2",
        "vue-jest": "^3.0.4",
        "vuepress": "^1.0.2"
    },
    "dependencies": {
        "highlight.js": "^9.15.8",
        "markdown-it-container": "^2.0.0"
    }
}
